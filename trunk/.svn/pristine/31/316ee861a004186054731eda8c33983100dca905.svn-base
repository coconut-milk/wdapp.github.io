<!doctype html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport"
        content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0">
  <meta http-equiv="X-UA-Compatible" content="ie=edge">
  <title>移动端观看直播</title>
</head>
<body>

<div class="app" id="app">
  <div class="top" id="top">
    <div class="main" id="main">
      <div class="video-wrapper">
        <div id="player">
          <!--视频-->
        </div>
      </div>
    </div>
    <div class="controls-wrapper" id="controlsWrapper">
      <div id="controls"></div>
    </div>
  </div>
  <div class="bottom" id="bottom">
    <div class="navigation">
      <div id="navigation">
        <!--导航-->
      </div>
    </div>
    <div class="swiper">
      <div class="swiper-container">
        <div class="swiper-wrapper">
          <div class="swiper-slide">
            <div id="document">
              <!--文档-->
            </div>
          </div>
          <div class="swiper-slide">
            <div id="chat">
              <!--聊天-->
            </div>
          </div>
          <div class="swiper-slide">
            <div id="questionAnswer">
              <!--问答-->
            </div>
          </div>
          <div class="swiper-slide">
            <div id="intro">
              <!--简介-->
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
  <div class="live-mobile-modal">
    <div id="setting"></div>
    <div id="privateMessage"></div>
  </div>
</div>

<script src="//view.csslcloud.net/js/jquery-1.9.0.min.js"></script>
<script
    src="//view.csslcloud.net/js/liveSDK.js?hash=d82f95001952f6e1c1a8"></script>
<script type="text/javascript" src="js/liveMobile.js?hash=16ad57a30de74a81832c"></script></body>
</html>